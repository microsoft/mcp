          "type": "string",
          "readOnly": false
        },
        "description": {
          "description": "The restore point description.",
          "type": "string",
          "readOnly": false
        },
        "creationMode": {
          "description": "The creation mode of the restore point. Additional creation mode types may be added over time.",
          "type": "string",
          "readOnly": true,
          "enum": [ "UserDefined", "SystemCreated" ],
          "x-ms-enum": {
            "name": "CreationModeType",
            "modelAsString": true,
            "values": [
              {
                "value": "UserDefined",
                "description": "User defined restore point."
              },
              {
                "value": "SystemCreated",
                "description": "System created restore point."
              }
            ]
          }
        },
        "creationDetails": {
          "description": "The creation metadata of the restore point.",
          "$ref": "#/definitions/RestorePointEventDetails"
        }
      }
