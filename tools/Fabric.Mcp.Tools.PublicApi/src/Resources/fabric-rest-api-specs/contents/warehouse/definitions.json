    "RestorePointEventDetails": {
      "type": "object",
      "description": "The restore point event details.",
      "required": [ "eventDateTime" ],
      "properties": {
        "eventDateTime": {
            "description": "The time of the restore point event in UTC, using the YYYY-MM-DDTHH:mm:ssZ format.",
