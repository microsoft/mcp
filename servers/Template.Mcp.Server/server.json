{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
  "name": "<<McpRepositoryName>>",
  "description": "Template MCP server example.",
  "version": "<<Version>>",
  "title": "Template MCP Server",
  "packages": [
    {
      "registryType": "nuget",
      "identifier": "<<DnxPackageId>>",
      "version": "<<Version>>",
      "transport": {
        "type": "stdio"
      },
      "packageArguments": [
        {
          "type": "positional",
          "value": "server"
        },
        {
          "type": "positional",
          "value": "start"
        }
      ]
    },
    {
      "registryType": "npm",
      "identifier": "<<NpmPackageName>>",
      "version": "<<Version>>",
      "transport": {
        "type": "stdio"
      },
      "packageArguments": [
        {
          "type": "positional",
          "value": "server"
        },
        {
          "type": "positional",
          "value": "start"
        }
      ]
    },
    {
      "registryType": "mcpb",
      "identifier": "<<McpbUrlWinX64>>",
      "fileSha256": "<<McpbSha256WinX64>>",
      "transport": {
        "type": "stdio"
      },
      "runtime": {
        "os": ["windows"],
        "arch": ["x86_64"]
      }
    },
    {
      "registryType": "mcpb",
      "identifier": "<<McpbUrlWinArm64>>",
      "fileSha256": "<<McpbSha256WinArm64>>",
      "transport": {
        "type": "stdio"
      },
      "runtime": {
        "os": ["windows"],
        "arch": ["arm64"]
      }
    },
    {
      "registryType": "mcpb",
      "identifier": "<<McpbUrlLinuxX64>>",
      "fileSha256": "<<McpbSha256LinuxX64>>",
      "transport": {
        "type": "stdio"
      },
      "runtime": {
        "os": ["linux"],
        "arch": ["x86_64"]
      }
    },
    {
      "registryType": "mcpb",
      "identifier": "<<McpbUrlLinuxArm64>>",
      "fileSha256": "<<McpbSha256LinuxArm64>>",
      "transport": {
        "type": "stdio"
      },
      "runtime": {
        "os": ["linux"],
        "arch": ["arm64"]
      }
    },
    {
      "registryType": "mcpb",
      "identifier": "<<McpbUrlOsxX64>>",
      "fileSha256": "<<McpbSha256OsxX64>>",
      "transport": {
        "type": "stdio"
      },
      "runtime": {
        "os": ["macos"],
        "arch": ["x86_64"]
      }
    },
    {
      "registryType": "mcpb",
      "identifier": "<<McpbUrlOsxArm64>>",
      "fileSha256": "<<McpbSha256OsxArm64>>",
      "transport": {
        "type": "stdio"
      },
      "runtime": {
        "os": ["macos"],
        "arch": ["arm64"]
      }
    }
  ],
  "repository": {
    "url": "https://github.com/microsoft/mcp",
    "source": "github",
    "subfolder": "servers/Template.Mcp.Server"
  },
  "websiteUrl": "https://github.com/microsoft/mcp/tree/main/servers/Template.Mcp.Server"
}