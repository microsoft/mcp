{
    "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
    "name": "<<McpRepositoryName>>",
    "description": "MCP tools for interacting with Microsoft Fabric",
    "version": "<<Version>>",
    "title": "Microsoft Fabric MCP Server",
    "packages": [
        {
            "registryType": "nuget",
            "identifier": "<<DnxPackageId>>",
            "version": "<<Version>>",
            "transport": {
                "type": "stdio"
            },
            "packageArguments": [
                {
                    "type": "positional",
                    "value": "server"
                },
                {
                    "type": "positional",
                    "value": "start"
                }
            ]
        },
        {
            "registryType": "npm",
            "identifier": "<<NpmPackageName>>",
            "version": "<<Version>>",
            "transport": {
                "type": "stdio"
            },
            "packageArguments": [
                {
                    "type": "positional",
                    "value": "server"
                },
                {
                    "type": "positional",
                    "value": "start"
                }
            ]
        }
    ],
    "repository": {
        "url": "https://github.com/microsoft/mcp",
        "source": "github",
        "subfolder": "servers/Fabric.Mcp.Server"
    },
    "websiteUrl": "https://github.com/microsoft/mcp/tree/main/servers/Fabric.Mcp.Server"
}