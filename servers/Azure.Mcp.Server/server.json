{
    "$schema": "https://static.modelcontextprotocol.io/schemas/2025-12-11/server.schema.json",
    "name": "<<McpRepositoryName>>",
    "description": "All Azure MCP tools to create a seamless connection between AI agents and Azure services.",
    "version": "<<Version>>",
    "title": "Azure MCP Server",
    "packages": [
        {
            "registryType": "nuget",
            "identifier": "<<DnxPackageId>>",
            "version": "<<Version>>",
            "transport": {
                "type": "stdio"
            },
            "packageArguments": [
                {
                    "type": "positional",
                    "value": "server"
                },
                {
                    "type": "positional",
                    "value": "start"
                }
            ]
        },
        {
            "registryType": "npm",
            "identifier": "<<NpmPackageName>>",
            "version": "<<Version>>",
            "transport": {
                "type": "stdio"
            },
            "packageArguments": [
                {
                    "type": "positional",
                    "value": "server"
                },
                {
                    "type": "positional",
                    "value": "start"
                }
            ]
        },
        {
            "registryType": "pypi",
            "identifier": "<<PypiPackageName>>",
            "version": "<<Version>>",
            "transport": {
                "type": "stdio"
            },
            "packageArguments": [
                {
                    "type": "positional",
                    "value": "server"
                },
                {
                    "type": "positional",
                    "value": "start"
                }
            ]
        },
        {
            "registryType": "mcpb",
            "identifier": "<<McpbUrlWinX64>>",
            "fileSha256": "<<McpbSha256WinX64>>",
            "transport": {
                "type": "stdio"
            }
        },
        {
            "registryType": "mcpb",
            "identifier": "<<McpbUrlWinArm64>>",
            "fileSha256": "<<McpbSha256WinArm64>>",
            "transport": {
                "type": "stdio"
            }
        },
        {
            "registryType": "mcpb",
            "identifier": "<<McpbUrlLinuxX64>>",
            "fileSha256": "<<McpbSha256LinuxX64>>",
            "transport": {
                "type": "stdio"
            }
        },
        {
            "registryType": "mcpb",
            "identifier": "<<McpbUrlLinuxArm64>>",
            "fileSha256": "<<McpbSha256LinuxArm64>>",
            "transport": {
                "type": "stdio"
            }
        },
        {
            "registryType": "mcpb",
            "identifier": "<<McpbUrlOsxX64>>",
            "fileSha256": "<<McpbSha256OsxX64>>",
            "transport": {
                "type": "stdio"
            }
        },
        {
            "registryType": "mcpb",
            "identifier": "<<McpbUrlOsxArm64>>",
            "fileSha256": "<<McpbSha256OsxArm64>>",
            "transport": {
                "type": "stdio"
            }
        }
    ],
    "repository": {
        "url": "https://github.com/microsoft/mcp",
        "source": "github",
        "subfolder": "servers/Azure.Mcp.Server"
    },
    "websiteUrl": "https://github.com/microsoft/mcp/tree/main/servers/Azure.Mcp.Server"
}